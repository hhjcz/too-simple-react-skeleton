{
  "name": "dohlestr-frontend",
  "version": "1.0.31",
  "repository": "git@bitbucket.org:hhjcz/dohlestr-frontend-scratch.git",
  "description": "hhj - redux/react dohlestr from scratch based on https://medium.com/front-end-developers/handcrafting-an-isomorphic-redux-application-with-love-40ada4468af4#.dyjo0n2px",
  "main": "index.js",
  "engines": {
    "node": "6.x"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "node --harmony ./src/server/index.js",
    "build": "node --harmony ./src/server/build.js",
    "dev": "docker/compose-wrapper.sh",
    "dev:up": "npm run dev up sshd workspace_node_dev",
    "dev:stop": "npm run dev stop",
    "dev:restart": "npm run dev restart",
    "dev:down": "npm run dev down",
    "dev:status": "npm run dev status",
    "dev:logs": "npm run dev -- logs -f",
    "dev:local": "NODE_ENV=development npm start",

    "pretest": "true || npm run eslint # (auto hook runs with npm run test)",
    "test": "mocha --compilers js:babel-register --require ./test_helper.js --reporter spec --recursive --watch-extensions jsx 'src/**/*.spec.js'",
    "test:watch": "npm run test -- --watch --reporter min",
    "eslint": "eslint --ext=js --ext=jsx src || true",
    "coverage": "istanbul cover -x '*.spec.*' _mocha -- --compilers js:babel-register --require ./test_helper.js --recursive --watch-extensions jsx 'src/**/*.spec.js'",
    "clean": "rm -r dist/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hhjcz/js-lib": "^1.0.3",
    "@hhjcz/react-lib": "^1.0.1",
    "@hhjcz/redux-rest": "^1.3.0",
    "autoprefixer": "^6.2.3",
    "babel-core": "^6.6.4",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.6.5",
    "babel-runtime": "^6.6.1",
    "bootstrap": "^3.3.6",
    "compression": "^1.6.1",
    "css-loader": "^0.25.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "history": "^4.3.0",
    "humps": "^1.0.0",
    "immutable": "^3.7.6",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "lodash": "^4.2.0",
    "material-ui": "^0.16.0",
    "node-sass": "^3.5.3",
    "postcss-loader": "^1.0.0",
    "query-string": "^4.0.2",
    "react": "^15.1.0",
    "react-addons-shallow-compare": "^15.1.0",
    "react-autocomplete": "^1.0.0-rc5",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-redux-toastr": "^3.0.5",
    "react-router": "^2.4.0",
    "react-router-bootstrap": "^0.24.2",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.0.5",
    "redux-localstorage": "^1.0.0-rc4",
    "redux-localstorage-debounce": "^0.1.0",
    "redux-logger": "^2.3.1",
    "redux-perf-middleware": "^1.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "webpack-isomorphic-tools": "^2.2.50"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "eslint": "^3.7.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^3.1.0",
    "nock": "^8.0.0",
    "react-addons-test-utils": "^15.1.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.10",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "skin-deep": "^0.16.0"
  }
}
