{
  "name": "dohlestr-frontend-scratch",
  "version": "1.0.0",
  "repository": "git@bitbucket.org:hhjcz/dohlestr-frontend-scratch.git",
  "description": "hhj - redux/react dohlestr from scratch based on https://medium.com/front-end-developers/handcrafting-an-isomorphic-redux-application-with-love-40ada4468af4#.dyjo0n2px",
  "main": "index.js",
  "scripts": {
    "start": "NODE_PATH=$NODE_PATH:./src/shared node --harmony index.js",
    "dev": "npm run start & webpack-dev-server --progress --color",
    "build": "NODE_ENV=production webpack --progress --color -p --config webpack.prod.config.js",
    "test": "mocha --compilers js:babel-core/register --require ./test_helper.js --recursive 'src/**/*.spec.js'",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.8.1",
    "express": "^4.13.3",
    "history": "1.13.x",
