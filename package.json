    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.12.0",
    "mocha": "^2.3.4",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
