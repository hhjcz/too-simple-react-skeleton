    "isomorphic-fetch": "^2.2.0",
    "object-assign": "^4.0.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.3",
    "react-router": "^1.0.2",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.2"
  },
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-core": "^6.3.21",
