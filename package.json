{
  "name": "dohlestr-frontend-scratch",
  "version": "1.0.1",
  "repository": "git@bitbucket.org:hhjcz/dohlestr-frontend-scratch.git",
  "description": "hhj - redux/react dohlestr from scratch based on https://medium.com/front-end-developers/handcrafting-an-isomorphic-redux-application-with-love-40ada4468af4#.dyjo0n2px",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production PORT=8080 NODE_PATH=$NODE_PATH:./src/shared node --harmony index.js",
    "prestart": "npm run build",
    "build": "NODE_ENV=production webpack --progress --color -p --config webpack.prod.config.js",
    "dev": "NODE_PATH=$NODE_PATH:./src/shared node --harmony index.js",
    "test": "mocha --compilers js:babel-register --require ./test_helper.js --recursive --watch-extensions jsx 'src/**/*.spec.js'",
    "test:watch": "npm run test -- --watch",
    "pretest": "npm run eslint # (auto hook runs with npm run test)",
    "eslint": "eslint --ext=js --ext=jsx src || true"
  },
